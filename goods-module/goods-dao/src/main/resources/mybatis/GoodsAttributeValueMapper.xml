<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.millinch.mall.goods.mapper.GoodsAttributeValueMapper">

	<resultMap id="BaseResultMap" type="com.millinch.mall.goods.entity.GoodsAttributeValue">
		<id column="id" property="id" />
		<result column="goods_id" property="goodsId" />
		<result column="code" property="code" />
		<result column="attribute_id" property="attributeId" />
		<result column="value" property="value" />
		<result column="value_image" property="valueImage" />
		<result column="initial" property="initial" />
		<result column="ordinal" property="ordinal" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, goods_id AS goodsId, code, attribute_id AS attributeId, value, value_image AS valueImage, initial, ordinal
    </sql>
</mapper>
