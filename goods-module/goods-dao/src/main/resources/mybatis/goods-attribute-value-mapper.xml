<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.millinch.mall.goods.mapper.GoodsAttributeValueMapper">
	<!--开启二级缓存-->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<resultMap id="BaseResultMap" type="com.millinch.mall.goods.entity.GoodsAttributeValue">
		<id column="id" property="id" />
		<result column="sku_id" property="id" />
		<result column="goods_id" property="id" />
		<result column="attribute_id" property="id" />
		<result column="value" property="value" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, sku_id AS id, goods_id AS id, attribute_id AS id, value
    </sql>
</mapper>
